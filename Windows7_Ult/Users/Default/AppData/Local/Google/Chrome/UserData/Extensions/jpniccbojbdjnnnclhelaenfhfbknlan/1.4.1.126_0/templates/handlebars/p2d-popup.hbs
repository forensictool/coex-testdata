<div class="_gis-p2d-form js_gis-p2d-form" style="">
    <div class="_gis-p2d-close js_gis-p2d-close"></div>
    <div class="_gis-p2d-header">
        <div class="_gis-p2d-h1">{{loc '__click_to_call'}}</div>
        <div class="_gis-p2d-form-step1">{{loc '__instant_call_on_your_smartphone'}}</div>
    </div>
    <div class="_gis-p2d-body _gis-p2d-form-step1 _gis-p2d-{{loc '__lang__'}}">
        <div class="_gis-p2d-desc">{{loc '__call_in_2_steps_description'}}</div>
        <dl>
            <dt><i class="first"></i>{{loc '__install_dialer_or_2gis'}}</dt>
            <dd>
                <div>
                    {{#locCond '__p2d-buttons-layout' '==' 'full'}}
                        <a href="http://ad.apps.fm/sXuVkpiWjZNe_8evMMskul5KLoEjTszcQMJsV6-2VnHFDLXitVHB6BlL95nuoNYfOpWTdDJOiqUWrNlyPerVT15CJQrDqOpiSSt4L1MzILz7mngc6xgMqEknxKWBUqZM"
                           data-ga-invoke="_P2DDialerDownload"
                           class="dialer-download-button js_gis-button-p2d"
                           target="_blank"><i class="_gis-icon-gisdialer"></i></a>
                        <a href="https://play.google.com/store/apps/details?id=ru.dublgis.dgismobile&utm_source=2gisforbrowsers&utm_medium=link&utm_campaign=android"
                           data-ga-invoke="_P2D2GisAndroidDownload"
                           class="gis-download-button js_gis-button-p2d"
                           target="_blank"><i class="_gis-icon-googleplay"></i>Google play</a>
                        <a href="http://itunes.apple.com/ru/app/2gis/id481627348?mt=8"
                           data-ga-invoke="_P2D2GisIosDownload"
                           class="gis-download-button js_gis-button-p2d"
                           target="_blank"><i class="_gis-icon-appstore"></i>App Store</a>
                        <div class="clearfix"></div>
                    {{/locCond}}
                    {{#locCond '__p2d-buttons-layout' '==' 'only-dialer'}}
                        <a href="http://ad.apps.fm/sXuVkpiWjZNe_8evMMskul5KLoEjTszcQMJsV6-2VnHFDLXitVHB6BlL95nuoNYfOpWTdDJOiqUWrNlyPerVT15CJQrDqOpiSSt4L1MzILz7mngc6xgMqEknxKWBUqZM"
                           data-ga-invoke="_P2DDialerDownload"
                           class="dialer-download-button dialer-download-button-single js_gis-button-p2ds"
                           target="_blank"><i class="_gis-icon-gisdialer"></i></a>
                        <div class="clearfix"></div>
                    {{/locCond}}
                </div>
            </dd>
            <dt><i class="second"></i>{{loc '__open_the_app'}}</dt>
            <dd>
                <form id="gis-p2d-form" class="">
                    <div>
                        <input type="text" name="pin[0]" value="" />
                        <input type="text" name="pin[1]" value="" />
                        <input type="text" name="pin[2]" value="" />
                        <input type="text" name="pin[3]" value="" />
                        <input type="text" name="pin[4]" value="" />
                        <input type="submit" value="{{loc '__link_phone'}}" class="_gis-p2d-submit" data-ga-invoke="_P2DConnectDevice" />
                    </div>
                    <div class="_gis-p2d-form-info">{{loc '__enter_pin'}}</div>
                </form>
            </dd>
        </dl>
    </div>
    <div class="_gis-p2d-body _gis-p2d-form-step2 _gis-p2d-{{loc '__lang__'}}">
        <div>{{loc '__your_phone_is_linked'}}</div>
        <input type="submit" value="{{loc '__check_it_out'}}" class="_gis-p2d-submit js_gis-p2d-call" />
        <div class="_gis-loader">
            <div class="loader241">
                <img src="data:image/svg+xml;base64," />
            </div>
            <div class="loader242">
                <img src="data:image/svg+xml;base64," />
            </div>
        </div>
    </div>
</div>

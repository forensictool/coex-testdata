<div data-ph-parsed="true" class="_gis-phone-highlight-widget{{#if firmData._woResult}} _gis-simple-widget{{/if}}{{#locCond '__lang__' '==' 'en'}} _gis-widget-layout-en{{/locCond}}" {{#if firmData._woResult}}data-wo-result="y"{{/if}}>
    {{#unless firmData._woResult}}
        <div data-ph-parsed="true" class="_gis-phone-highlight-w-close js_gis-phone-highlight-close"></div>
        <div data-ph-parsed="true" class="_gis-phone-highlight-header">
            <div data-ph-parsed="true" class="_gis-phone-highlight-title">
                {{#if firmData.firmBigMapLink}}
                    <a data-ph-parsed="true" target="_blank" href="{{firmData.firmBigMapLink}}" data-reg-bc-url="{{firmData.reg_bc_url}}">
                {{/if}}
                    {{#if firmData.firmNameParts.primary}}
                        {{firmData.firmNameParts.primary}}
                    {{else}}
                        {{firmData.firmName}}
                    {{/if}}
                    {{#if firmData.firmNameParts.extension}}
                        <div data-ph-parsed="true">{{firmData.firmNameParts.extension}}</div>
                    {{/if}}
                {{#if firmData.firmBigMapLink}}
                    </a>
                {{/if}}
            </div>

            {{#if firmData.reviews}}
            <div data-ph-parsed="true" class="_gis-rating">
                <div data-ph-parsed="true" class="_gis-comments">
                    {{#if firmData.firmFlampRating}}
                        <div data-ph-parsed="true" class="_gis-total-stars">
                            <div data-ph-parsed="true" class="_gis-stars-active" style="width: {{firmData.firmFlampRating}}%"></div>
                        </div>
                    {{/if}}
                    <a data-ph-parsed="true" target="_blank" data-ga-invoke="_PHReviewsClick" href="{{firmData.reviews.flampReviewsLink}}" data-reg-bc-url="{{firmData.reviews.reg_bc_url}}">{{{reviewsString}}}</a>
                </div>

                {{#if firmData.firmFlampRecommendationCount}}
                    <div data-ph-parsed="true" class="_gis-recommends">
                        <span data-ph-parsed="true">{{{recommendationString}}}</span>
                    </div>
                {{/if}}
            </div>
            {{/if}}

        </div>
    {{/unless}}

    <div data-ph-parsed="true" class="{{#if firmData._woResult}}_gis-phone-highlight-body-empty{{else}}_gis-phone-highlight-body{{/if}}">
    {{#if todayDesc}}
        <div data-ph-parsed="true" class="_gis-phone-highlight-section _gis-phone-highlight-work-mode {{#unless todayDesc.isOpen}} _gis-phone-highlight-close{{/unless}}">
            {{#unless todayDesc.allTime}}
                <a data-ph-parsed="true" data-ga-invoke="_PHScheduleClick" class="_gis-phone-highlight-expand _gis-phone-highlight-opened js_gis-phone-highlight-schedule-link" href="javascript:;">{{todayDesc.todayText}}</a>
            {{else}}
                <a data-ph-parsed="true" data-ga-invoke="_PHScheduleClick" class="_gis-phone-highlight-expand _gis-phone-highlight-no-active" href="javascript:;">{{todayDesc.todayText}}</a>
            {{/unless}}
            <div data-ph-parsed="true" class="_gis-phone-highlight-info">
                {{todayDesc.todayState}}
            </div>

            {{#unless todayDesc.allTime}}
                {{#if shortSchedule}}
                    <div data-ph-parsed="true" class="_gis-phone-highlight-schedule-popup _gis-phone-highlight-simple js_gis-phone-highlight-schedule-popup">
                        <div data-ph-parsed="true" class="_gis-phone-highlight-arr"></div>
                        {{#each shortSchedule}}
                            <div data-ph-parsed="true">{{this.name}} {{{this.value}}}</div>
                        {{/each}}
                    </div>
                {{else}}
                    <div data-ph-parsed="true" class="_gis-phone-highlight-schedule-popup js_gis-phone-highlight-schedule-popup">
                        <div data-ph-parsed="true" class="_gis-phone-highlight-arr"></div>
                        <div data-ph-parsed="true" class="_gis-phone-highlight-ico">
                            <div data-ph-parsed="true" class="_gis-phone-highlight-working"></div> <div class="_gis-phone-highlight-break"></div>
                        </div>
                    </div>
                {{/if}}
            {{/unless}}
        </div>
    {{/if}}

    {{#each firmData.firmContactGroups}}
        {{! this = group }}
        {{#if this.name}}
            <h2>{{this.name}}</h2>
        {{/if}}
        {{#each this.contacts}}
            {{! this = contact }}
            {{#ifCond @key '==' 'email'}}
                {{#each this}}
                    <div data-ph-parsed="true" class="_gis-phone-highlight-section _gis-phone-highlight-{{this.type}}"><a href="mailto:{{this.link}}" data-reg-bc-url="{{this.reg_bc_url}}">{{this.link}}</a></div>
                {{/each}}
            {{else}}{{#ifCond @key '==' 'website'}}
                {{#each this}}
                    {{#ifCond this.alias '!=' @firmData.currDomain}}
                        <div data-ph-parsed="true" class="_gis-phone-highlight-section _gis-phone-highlight-{{this.type}}">
                            <a data-ph-parsed="true" href="{{this.link}}" target="_blank" data-reg-bc-url="{{this.reg_bc_url}}">{{this.alias}}</a>
                        </div>
                    {{/ifCond}}
                {{/each}}
            {{else}}{{#ifCond @key 'in' 'phone,fax'}}
                {{#each this}}
                    <div data-ph-parsed="true" class="_gis-phone-highlight-section _gis-phone-highlight-phone js_gis-phone _gis-phone-highlight-clearfix _gis-phone-show-opened">
                        <i data-ph-parsed="true" class="_gis-icon _gis-icon-phone"></i>
                        {{#if @firmData._woResult}}
                            <div data-ph-parsed="true" class="_gis-phone-wrapper _gis-phone-highlight-clearfix">
                                <a data-ph-parsed="true" href="javascript:;" class="_gis-p2d-link js_gis-p2d-link"
                                   data-reg-bc-url="{{this.reg_bc_url}}"
                                   data-value="{{addCountryCode this.link}}"
                                   data-description='{{loc "__dialing_to_{number}" this.alias}}'>
                                    <span data-ph-parsed="true">{{loc '__dial'}}</span>
                                </a>
                                {{#if this.comment}}
                                    <span data-ph-parsed="true" class="_gis-p2d-link-info">{{this.comment}}</span>
                                {{/if}}
                                <span data-ph-parsed="true" class="_gis-p2d-error js_gis-p2d-error"></span>
                            </div>
                            <div data-ph-parsed="true" class="push2dial-message">
                                <div data-ph-parsed="true" class="p2d-preloader">
                                    <span data-ph-parsed="true" class="p2d-preloader__dots"></span>
                                    <span data-ph-parsed="true" class="p2d-preloader__cellphone-icon"></span>
                                </div>
                            </div>
                        {{else}}
                            <div data-ph-parsed="true" class="_gis-phone-wrapper _gis-phone-highlight-clearfix">
                                <a data-ph-parsed="true" href="javascript:;" class="_gis-p2d-link js_gis-p2d-link"
                                   data-reg-bc-url="{{this.reg_bc_url}}"
                                   data-value="{{addCountryCode this.link}}"
                                   data-description='{{loc "__dialing_to_{firmName}" @firmData.firmName}}'>
                                    <span data-ph-parsed="true">{{this.alias}}</span>
                                </a>
                                <div data-ph-parsed="true" class="push2dial-message">
                                    <div data-ph-parsed="true" class="p2d-preloader">
                                        <span data-ph-parsed="true" class="p2d-preloader__dots"></span>
                                        <span data-ph-parsed="true" class="p2d-preloader__cellphone-icon"></span>
                                    </div>
                                </div>
                                {{#if this.comment}}
                                    <span data-ph-parsed="true" class="_gis-p2d-link-info">{{this.comment}}</span>
                                {{/if}}
                                <span data-ph-parsed="true" class="_gis-p2d-error js_gis-p2d-error"></span>
                            </div>
                        {{/if}}
                    </div>
                {{/each}}
            {{else}}
                {{#each this}}
                    <div data-ph-parsed="true" class="_gis-phone-highlight-section _gis-phone-highlight-{{this.type}}">
                        {{this}} {{! todo: really? }}
                    </div>
                {{/each}}
            {{/ifCond}}{{/ifCond}}{{/ifCond}}
        {{/each}}
    {{/each}}
    {{#if firmData.firmBigMapLink}}
        <div data-ph-parsed="true" class="_gis-phone-highlight-section _gis-phone-highlight-moreinfo">
            <a data-ph-parsed="true" href="{{firmData.firmBigMapLink}}" target="_blank" data-ga-invoke="_PHMoreClick" data-reg-bc-url="{{firmData.reg_bc_url}}">
                {{#ifCond firmData.provider '==' 'gmaps'}}
                    {{loc '__get_more_at_{source}' 'Google'}}
                    <img style="float: right" src="data:image/png;base64," />
                {{else}}
                    {{loc '__get_more_at_{source}' '2GIS'}}
                {{/ifCond}}
            </a>
        </div>
    {{/if}}

    </div>
</div>
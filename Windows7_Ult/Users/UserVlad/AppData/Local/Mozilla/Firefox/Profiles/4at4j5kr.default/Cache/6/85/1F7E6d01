  <name>SQLite Manager</name>
  <type id="1">Расширение</type>
  <guid>SQLiteManager@mrinalkant.blogspot.com</guid>
  <slug>sqlite-manager</slug>
  <version>0.8.1</version>
  <status id="4">Полностью проверен</status>
  <authors>
        <author id="237862">
